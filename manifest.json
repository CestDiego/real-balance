{
  "manifest_version": 2,
  "name": "Real Balance",
  "version": "0.1",

  "icons": {
    "128": "icon.png"
  },

  "browser_action":{
    "default_icon": "icon.png"
  },

  "content_scripts": [
    {
      "matches": ["https://secure.bankofamerica.com/*"],
      "js": ["script.js"]
    }
  ],

  "permissions": [
    "tabs",
    "https://secure.bankofamerica.com/*"
  ]
}
